{
  "name": "unveil-network-sync",
  "version": "0.0.1",
  "description": "Network sync for unveil",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irisSchaffer/unveil-network-sync.git"
  },
  "keywords": [
    "unveil",
    "presentation",
    "web",
    "sockets"
  ],
  "author": "Iris Schaffer <irisSchaffer@gmx.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/irisSchaffer/unveil-network-sync/issues"
  },
  "homepage": "https://github.com/irisSchaffer/unveil-network-sync#readme",
  "dependencies": {
    "unveil": "^0.1.2"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.8.2",
    "react-addons-test-utils": "^0.14.6",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify",
      "reactify"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/styles/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathIgnorePatterns": [
      "fixtures"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "fixtures"
    ]
  },
  "scripts": {
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > build/bundle.js; sass --update ./styles:./build",
    "start": "watchify -t [envify --NODE_ENV local] -o build/bundle.js -v -d src/app.js & sass -w ./styles:./build",
    "test": "jest"
  }
}
